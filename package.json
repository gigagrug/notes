{
  "name": "blog",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "tw": "npx tailwindcss -i ./src/assets/style.css -o ./src/assets/build.css --minify --watch",
